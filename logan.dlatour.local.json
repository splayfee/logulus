{
  "baseName": "development",
  "transports": [
    {"name":"console", "colorize": true, "handleExceptions": true, "silent": false},
    {"name":"file", "saveCount": 5, "handleExceptions": true, "silent": false}
  ],
  "includeFilters": [
    {"pattern": "*", "levels": ["debug", "info", "warn", "error"]}
  ],
  "timestampSettings": {"includeDate": true, "includeMilliseconds": true},
  "showModule": true,
  "colors": {
    "debug": "green",
    "info": "green",
    "warn": "yellow",
    "error": "red"
  },
  "levels": {
    "debug": 0,
    "info": 1,
    "warn": 2,
    "error": 3
  },
  "exitOnError": false
}